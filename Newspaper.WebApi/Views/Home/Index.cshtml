@{
    ViewData["Title"] = "Home Page";
}

@model List<Haber>


<div class="carousel-settings" style="width:80%;margin-left:auto;margin-right:auto;">
    <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel" style="margin-top: 12px;">

        <ol class="carousel-indicators">
            <li data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active"></li>

                        <h5>First slide label</h5>
                        <p>Nulla vitae elit libero, a pharetra augue mollis interdum.</p>
                    </a>
                </div>
            </div>

        </div>
        <a class="carousel-control-prev" href="#carouselExampleCaptions" role="button" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleCaptions" role="button" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </a>
    </div>
</div>

<h2>Trend <span class="badge bg-primary" style="margin-top: 20px; margin-bottom:12px;">Haberler</span></h2>
<div class="row" style="margin-top: 12px;">
     @foreach (var item in Model)
     {
    <div class="col-sm-6" style="margin-bottom: 8px;">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">@item.Baslik</h5>
                <p class="card-text">@item.Metin</p>
                <a asp-controller="Home" asp-action="Details" class="stretched-link"></a>
            </div>
        </div>
    </div>
     }

</div>


<h2>Yeni <span class="badge bg-primary" style="margin-top: 20px; margin-bottom:12px;">Haberler</span></h2>

<div class="card-group" style="margin-top: 6px;">
    @for (int i = 0; i < 6; i++)
    {
    <div class="card" style="border-radius: 2%;">
        <img src="https://valmorganshowcase.com.au/wp-content/uploads/2019/02/placeholder-150x150-300x300.jpg" class="card-img-top" alt="...">
        <div class="card-body">
            <h5 class="card-title">Card title</h5>
            <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
            <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
            <a asp-controller="Home" asp-action="Details" class="stretched-link"></a>
        </div>
    </div>
    }

</div>


